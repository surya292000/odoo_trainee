<?xml version="1.0" encoding="UTF-8" ?>
<odoo>


    <record id="example_line_view_list" model="ir.ui.view">
        <field name="name">example.line.view.list</field>
        <field name="model">example.line</field>
        <field name="arch" type="xml">
            <list editable="bottom">



                <field name="product_id"/>
                <field name="price"/>
                <field name="qty"/>
                <field name="sub_total"/>
                <field name="example_id"/>



            </list>
        </field>
    </record>
      <record id="action_example_line" model="ir.actions.act_window">
        <field name="name">Example Line</field>
        <field name="res_model">example.line</field>
        <field name="view_mode">list</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face"></p>
        </field>

    </record>



</odoo>
