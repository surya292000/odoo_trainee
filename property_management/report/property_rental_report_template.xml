<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="property_rental_report_template">
        <t t-call="web.html_container">
            <!--            <t t-foreach="docs" t-as="o">-->
            <t t-call="web.external_layout">
                <h1>sdfghjkl</h1>
                <t t-esc="o"/>
                <div class="page">
                    <h2 style="font-size:55px">Property Report</h2>
                    <br/>
                    <table class="o_has_total_table table o_main_table table-borderless">
                        <thead t-attf-style="font-size: 24px">
                            <tr>
                                <th name="th_sequence" class="text-start">Rental Lease Id</th>
                                <th name="th_owner" class="text-nowrap">Owner</th>
                                <th name="th_property" class="text-nowrap">Property</th>
                                <th name="th_type" class="text-nowrap">Property Type</th>
                                <th name="th_tenant" class="text-nowrap">Tenant</th>
                                <th name="th_start_date" class="text-nowrap">Start Date</th>
                                <th name="th_end_date" class="text-nowrap">End Date</th>
                                <th name="th_amount" class="text-nowrap">Amount</th>
                                <th name="th_state" class="text-nowrap">State</th>
                            </tr>
                        </thead>
                        <tbody t-attf-style="font-size: 24px">
                            <t t-foreach="data" t-as="line">
                                <tr>
                                    <td name="td_sequence"><span t-esc="line['rental_lease_id']"> </span></td>
                                    <td name="td_property">
                                        <span t-esc="line['property']"/>
                                    </td>
                                    <td name="th_owner">
                                        <span t-esc="line['owner_name']"/>
                                    </td>
                                    <td name="th_type">
                                        <span t-esc="line['type']"/>
                                    </td>
                                    <td name="th_tenant">
                                        <span t-esc="line['tenant_name']"/>
                                    </td>
                                    <td name="th_start_date">
                                        <span t-esc="line['start_date']"/>
                                    </td>
                                    <td name="th_end_date">
                                        <span t-esc="line['end_date']"/>
                                    </td>
                                    <td name="th_amount">
                                        <span t-esc="line['amount']"/>
                                    </td>
                                    <td name="th_state">
                                        <span t-esc="line['state']"/>
                                    </td>
                                </tr>
                            </t>
                        </tbody>
                    </table>
                </div>
            </t>
        </t>
    </template>
</odoo>

