<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="property_rental_lease_report_template">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <div class="page">
                    <h2>My Model Report</h2>
                    <p>
                        <strong>Name:</strong>
                        <t t-esc="doc.name"/>
                    </p>
                    <p>
                        <strong>Description:</strong>
                        <t t-esc="doc.description"/>
                    </p>
                </div>
            </t>
        </t>
    </template>
</odoo>
