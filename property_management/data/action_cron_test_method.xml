<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
        <record id="cron_expire_leases" model="ir.cron">
            <field name="name">Auto Expire Leases</field>
            <field name="model_id" ref="property_management.model_property_rental_lease"/>
            <field name="state">code</field>
            <field name="code">model._cron_expire_leases()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
        </record>
    <record id="cron_late_payment" model="ir.cron">
        <field name="name">Auto Late Payment Follow Up</field>
        <field name="model_id" ref="property_management.model_property_rental_lease"/>
        <field name="state">code</field>
        <field name="code">model._cron_late_payment()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
    </record>
     <record id="cron_archive_leases" model="ir.cron">
            <field name="name">Auto Archive Leases</field>
            <field name="model_id" ref="property_management.model_property_rental_lease"/>
            <field name="state">code</field>
            <field name="code">model._cron_archive_leases()</field>
            <field name="interval_number">2</field>
            <field name="interval_type">days</field>
        </record>
</odoo>
