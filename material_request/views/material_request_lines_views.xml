<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="material_request_lines_view_list" model="ir.ui.view">
        <field name="name">material.request.lines.view.list</field>
        <field name="model">material.request.lines</field>
        <field name="arch" type="xml">
            <list>
                <field name="component_id"/>
                <field name="quantity" string="Quantity"/>
                <field name="request_id"/>
                <field name="vendor_ids"/>
                <field name="request_type"/>
            </list>
        </field>
    </record>
    <record id="material_request_lines_view_form" model="ir.ui.view">
        <field name="name">material.request.lines.view.form</field>
        <field name="model">material.request.lines</field>
        <field name="arch" type="xml">
            <form>
                <field name="component_id"/>
                <field name="quantity" string="Quantity"/>
                <field name="vendor_ids" />
                <field name="request_type"/>
                <field name="src_location_id"/>
                <field name="dest_location_id"/>
            </form>
        </field>
    </record>
</odoo>
